{
  "compilerOptions": {
    "module": "ES2015",
    "target": "ESNext",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "paths": {
      "@servarr-api": ["./src/servarr-api/index.ts"],
      "@servarr-api/*": ["./src/servarr-api/*"],
      "@tasks/*": ["./src/tasks/*"],
      "@src/*": ["./src/*"]
    }
  },
  "include": ["src/**/*"]
}
